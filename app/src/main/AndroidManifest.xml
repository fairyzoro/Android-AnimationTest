<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bob_jiang.animationtest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PropertyAnimation"></activity>

        <activity android:name=".DrawableAnimation"></activity>

        <activity android:name=".ViewAnimation"></activity>

        <activity android:name=".DrawableCanvasAnimation"></activity>

        <activity android:name=".SurfaceViewAnimation"></activity>

    </application>

</manifest>
